apiVersion: v1
kind: Pod
metadata:
  name: nginx
  labels:
    app.kubernetes.io/name: proxy
spec:
  containers:
  - name: nginx
    image: nginx:latest
    ports:
      - containerPort: 80
        name: http-web-svc
====================================================

kubectl api-resources
kubectl explain pods
kubectl config set-context --current --namespace=<namespace-name>
